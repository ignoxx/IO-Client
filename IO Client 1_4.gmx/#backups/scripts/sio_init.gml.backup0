gml_pragma("global", "sio_init()");

//Global vars
global.ip = "localhost";
global.port = 3000;

// Enums
enum NetOP {
    None = "none",
    Register = "register",
    QuickPlay = "quick_play"
}

//SocketIO
sio_connect(global.ip, global.port);

//Add events
sio_addEvent(NetOP.QuickPlay);
sio_addEvent(NetOP.Register);
